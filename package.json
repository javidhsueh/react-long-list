{
  "name": "react-long-list",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist",
    "mkdir": "mkdir -p ./dist/assets/css && mkdir -p ./dist/assets/imgs/",
    "copy": "cp ./src/views/* ./dist/ ",
    "set": "npm run clean && npm run mkdir && npm run copy",
    "start": "npm run set && NODE_ENV=development node ./bin/dev",
    "production": "npm run set && NODE_ENV=production webpack --config webpack.config.prod.js && NODE_ENV=production node ./bin/www",
    "no-abstract": "npm run set && NODE_ENV=production webpack --config webpack.config.noAbstract.js && NODE_ENV=production node ./bin/www",
    "redux": "npm run set && NODE_ENV=production webpack --config webpack.config.redux.js && NODE_ENV=production node ./bin/www",
    "dev": "npm run set && webpack --config webpack.config.dev.js && NODE_ENV=production node ./bin/www"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "history": "^3.0.0",
    "jsdom": "^9.4.0",
    "lodash": "^4.13.1",
    "morgan": "^1.7.0",
    "normalize.css": "^4.2.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "recompose": "^0.20.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.2",
    "react-addons-perf": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "redbox-react": "^1.2.10",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.19",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.0.0-beta",
    "webpack-hot-middleware": "^2.12.1"
  }
}
